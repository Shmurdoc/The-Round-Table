# Root .gitignore for The Round Table project

# OS files
Thumbs.db
ehthumbs.db
.DS_Store

# Logs and databases
*.log
*.sql
*.sqlite

# PHP / Composer
/vendor/
/composer.phar
/composer.lock

# Node
/node_modules/
/npm-debug.log*
/yarn-error.log*

# Environment files
*.env
.env.*

# IDEs and editors
/.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Build outputs
/public/build/
/public/dist/
/mix-manifest.json

# Laravel / framework specific
/storage/
/public/storage/
.env
Homestead.yaml
Homestead.json
/public/hot
/bootstrap/cache/*.php

# Ignore OS-specific WAMP artifacts
wwroot/

# IntelliJ
*.iml

# Archives and backups
*.zip
*.tar.gz
*.rar
*.bak

# Ignore uploaded user data and KYC images
The Table/public/storage/
The Table/storage/

# Allow gitkeep files
!The Table/storage/.gitignore
!The Table/public/storage/.gitignore
